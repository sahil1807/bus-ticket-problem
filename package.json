{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "type": "module",
  "description": "Node.js boilerplate for any simple node.js project",
  "main": "src/bin/www.js",
  "engines": {
    "node": ">=10.20.1",
    "npm": ">=6.14.4"
  },
  "engine-strict": true,
  "scripts": {
    "start": "nodemon",
    "inspect": "nodemon --inspect src/bin/www.js",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint — fix”, “git add"
    ]
  },
  "keywords": [],
  "author": "Sahil Arora",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "celebrate": "^9.1.0",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "http-status-codes": "^1.4.0",
    "lint-staged": "^10.1.2",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.27",
    "mongoose": "^5.10.7",
    "morgan": "^1.10.0",
    "morgan-body": "^2.4.8",
    "multer": "^1.4.2",
    "node-cache": "^5.1.0",
    "node-cmd": "^3.0.0",
    "pg": "^7.13.0",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.0.0",
    "jest": "latest",
    "prettier": "^1.17.1"
  },
  "bugs": {},
  "homepage": ""
}
